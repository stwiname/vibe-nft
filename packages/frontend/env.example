# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id_here

# Contract addresses (update after deployment)
NEXT_PUBLIC_CONTRACT_ADDRESS_BASE=your_base_mainnet_contract_address_here
NEXT_PUBLIC_CONTRACT_ADDRESS_BASE_SEPOLIA=your_base_sepolia_contract_address_here

# Required ERC-20 Token Addresses (for minting requirement)
NEXT_PUBLIC_REQUIRED_TOKEN_ADDRESS=your_erc20_token_address_here
NEXT_PUBLIC_REQUIRED_TOKEN_ADDRESS_BASE=your_base_mainnet_erc20_token_address_here
NEXT_PUBLIC_REQUIRED_TOKEN_ADDRESS_BASE_SEPOLIA=your_base_sepolia_erc20_token_address_here

# RPC URLs
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# OpenAI API Key for image generation (get from https://platform.openai.com/)
OPENAI_API_KEY=your_openai_api_key_here

# Pinata JWT Key for IPFS uploads (get from https://app.pinata.cloud/)
PINATA_JWT_KEY=your_pinata_jwt_key_here

# Private key for signature generation (contract owner's private key)
PRIVATE_KEY=your_private_key_here

# Etherscan v2 API Key (for contract verification on all networks, including Basescan)
ETHERSCAN_API_KEY=your_etherscan_v2_api_key_here 